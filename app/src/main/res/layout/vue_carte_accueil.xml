<?xml version="1.0" encoding="utf-8"?>
<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vue.vueCarteAccueil">

    <Button
    android:id="@+id/vue_carte_action_naviguer_liste"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal|top"
    android:text="Voir la liste des événements"
    android:padding="10dp"
    android:layout_marginTop="20dp"
    android:paddingRight="10dp"/>

    <Button
        android:id="@+id/vue_carte_action_naviguer_ajout_evenement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|top"
        android:text="Ajouter un événement"
        android:padding="10dp"
        android:layout_marginTop="60dp"
        android:paddingRight="10dp"/>

    </fragment>
